{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run generate:api && vite",
    "build": "npm run generate:api && tsc -b && vite build",
    "build:ci": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test --project=mocked",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:real": "playwright test --project=real-backend",
    "test:e2e:mocked": "playwright test --project=mocked",
    "test:e2e:debug": "playwright test --debug",
    "generate:api": "openapi-generator-cli generate -i http://localhost:8080/api/api-docs -g typescript-axios -o src/generated/api --additional-properties=typescriptThreePlus=true,supportsES6=true,withInterfaces=true,npmName=@skysync/api-client,npmVersion=1.0.0",
    "generate:api:prod": "openapi-generator-cli generate -i https://skysync.christianzhou.com/api/api-docs -g typescript-axios -o src/generated/api --additional-properties=typescriptThreePlus=true,supportsES6=true,withInterfaces=true,npmName=@skysync/api-client,npmVersion=1.0.0",
    "generate:api:local": "openapi-generator-cli generate -i ./api-spec.json -g typescript-axios -o src/generated/api --additional-properties=typescriptThreePlus=true,supportsES6=true,withInterfaces=true,npmName=@skysync/api-client,npmVersion=1.0.0",
    "fetch:api-spec": "curl http://localhost:8080/api/api-docs > api-spec.json",
    "fetch:api-spec:prod": "curl https://skysync.christianzhou.com/api/api-docs > api-spec.json"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/lab": "^7.0.1-beta.18",
    "@mui/material": "^7.3.4",
    "@mui/x-tree-view": "^8.14.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-resizable-panels": "^3.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "axios": "^1.6.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "jsdom": "^24.0.0",
    "@playwright/test": "^1.40.0",
    "msw": "^2.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7",
    "vitest": "^2.0.0"
  }
}
